{
  "settings": {
  },
  "mappings": {
    "trace": {
      "_ttl": {
        "enabled": true,
        "default": "15d"
      },
      "_all": {
        "enabled": false
      },
      "properties": {
        "date": {
          "type": "date"
        },
        "id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "app": {
          "type": "string",
          "index": "not_analyzed"
        },
        "result": {
          "type": "string",
          "index": "not_analyzed"
        },
        "action": {
          "type": "string",
          "index": "not_analyzed"
        },
        "content": {
          "type": "string"
        }
      }
    }
  }
}