{
  "template": "stat-*",
  "order": 1,
  "settings": {
    "number_of_shards": 1
  },
  "mappings": {
    "stat": {
      "_all": {
        "enabled": false
      },
      "properties": {
        "date": {
          "type": "date"
        },
        "id": {
          "type": "keyword"
        },
        "app": {
          "type": "keyword"
        },
        "server_ip": {
          "type": "ip"
        }
      }
    }
  }
}